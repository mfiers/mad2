checksum:
  enabled: true
  module: mad2.plugin.checksum
samplesheet:
  enabled: true
  module: mad2.plugin.samplesheet
onthefly:
  enabled: true
  module: mad2.plugin.onthefly
set:
  enabled: true
  module: mad2.plugin.madset
find:
  enabled: true
  module: mad2.plugin.find
  ignore:
    - QDSUMS
    - SHA1SUMS
select:
  enabled: true
  module: mad2.plugin.select
table:
  enabled: true
  module: mad2.plugin.table
print:
  enabled: true
  module: mad2.plugin.print
provenance:
  enabled: true
  module: mad2.plugin.provenance
backup:
  enabled: true
  module: mad2.plugin.backup
cpmv:
  enabled: true
  module: mad2.plugin.cpmv
template:
  enabled: true
  module: mad2.plugin.template
  group_template: |
    {%- for e in elements %}
    {{ e }}
    {%- endfor %}
qdcheck:
  enabled: true
  module: mad2.plugin.qdcheck
x:
  enabled: false
  module: mad2.plugin.x
  default_executor: simple
  defaults:
    annotate:
      createdBy: "$(id -un)@$(hostname)"
      createdHost: "$(hostname)"
      createdOn: '$(date  +"%Y-%m-%dT%H:%M:%S")'
neo4j:
  enabled: false
  autostore: false
  module: mad2.plugin.neo
  uri: 'http://localhost:7474/db/data/'
root:
  enabled: true
  module: mad2.plugin.root
lcbhacks:
  enabled: false
  module: mad2.plugin.lcbhacks
category:
  enabled: true
  module: mad2.plugin.category
mongo:
  module: mad2.plugin.mongo
  host: localhost
  db: mad2
  collection: dump
  enabled: false
  indici:
    - project
    - save_time
    - userid
    - sha1
    - username
    - host
    - filesize
    - dirname
    - category
    - extension
    - basename
